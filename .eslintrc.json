{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/require-default-props": 0,
    "max-len": "off",
    "no-nested-ternary": "off",
    "no-array-index-key": "off",
    "class-methods-use-this": "off"
  },
  "globals": {
    "document": true,
    "window": true,
    "localStorage": true,
    "fetch": true
  }
}
